<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>

    <%- include('nav.ejs') %>
    <% result.forEach(element=>{ %>

       
        <img src="<%= element.image%>" alt="">
        <h3>Name: <%= element.name%> </h3>
        <h3> Patronus:<%= element.patronus%> </h3>
        <h3> Alive:<%= element.alive %> </h3>
   
         <form action="/my-characters" method="POST">
        <input type="hidden" name="name" value="<%= element.name%>">
        <input type="hidden" name="patronus" value="<%= element.patronus%>">
        <input type="hidden" name="alive" value="<%= element.alive%>">
         <button type="submit">Add to Database</button>
        </form>
   <% })%>
</body>
</html>